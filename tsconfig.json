{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strict": false,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "DOM"],
    "types": ["node"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
